@model IEnumerable<DatabaseAccess.TimeTblTable>

@{
    ViewBag.Title = "Index";
}

<p>
    @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-success" })
</p>
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">All Annual Fee</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.StartTime)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.EndTime)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Day)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.IsActive)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.ClassSubjectTable.Title)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.ClassTable.Name)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.RoomTable.Title)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.SessionProgrameSubjectSettingTable.Description)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.StaffTable.Name)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.SubjectTable.Name)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.UserTable.FullName)
                        </th>
                        <th>
                            Action
                        </th>
                    </tr>
                    </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.StartTime)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.EndTime)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Day)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.IsActive)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ClassSubjectTable.Title)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ClassTable.Name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.RoomTable.Title)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.SessionProgrameSubjectSettingTable.Description)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.StaffTable.Name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.SubjectTable.Name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.UserTable.FullName)
                            </td>
                            <td class="btn-group">
                                @Html.ActionLink("Edit", "Edit", new { id = item.TimeTableID }, new { @class = "btn btn-primary" })
                                @Html.ActionLink("Details", "Details", new { id = item.TimeTableID }, new { @class = "btn btn-info" })
                                @Html.ActionLink("Delete", "Delete", new { id = item.TimeTableID }, new { @class = "btn btn-danger" })
                            </td>
                        </tr>
                    }
                </tbody>

            </table>

            </div>
        </div>
    </div>
